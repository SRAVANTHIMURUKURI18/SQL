19)
select distinct dname
    from dept,emp
    where emp.deptno=dept.deptno and sal>(select avg(sal)
                                           from emp);

20)
select ename
    from emp
    where ename like '_____';

21)
select ename
    from emp
    where ename like 'J%_S';

22)
select ename
    from emp
    where deptno!=10 and deptno!=20 and deptno!=40;
    
23)
select ename
    from emp
    where job!='PRESIDENT' and job!='MANAGER';

24)
select max(sal)
    from emp;

25)
select max(sal)
    from emp
    where job='SALESMAN';

26)
select ename
    from emp
    where ename like '_L%_' or ename like '_%l';

27)
28)
29)
select sum(sal),dname
    from emp,dept
    where emp.deptno=dept.deptno
    group by dname;

30)
select dname dept,min(sal) min,max(sal) max
    from emp,dept
    where emp.deptno=dept.deptno
    group by dname;

31)
