1)
select *
from emp;

2)
select *
from emp
where deptno=10;

3)
select *
from emp
where deptno!=30;

4)
select count(*)
from emp;

5)
select *
from emp
order by sal desc;

6)SELECT * FROM emp FETCH FIRST 5 ROWS ONLY;

7)
select *
from emp
order by deptno,sal desc;


8)
select *
from emp
where hiredate between '31-DEC-80' and '01-JAN-82';

9)



10)
